@font-face {
    font-family StudioScriptC
    src: local('StudioScriptCTT'), local('StudioScriptC'),
        url('fonts/subset-StudioScriptC.woff2') format('woff2'),
        url('fonts/subset-StudioScriptC.woff') format('woff'),
        url('fonts/subset-StudioScriptC.svg#StudioScriptC') format('svg');
    font-weight: normal;
